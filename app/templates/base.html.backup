    <!-- Clean Navigation JavaScript -->
    <script>
    document.addEventListener('DOMContentLoaded', function() {
        console.log('🚀 New clean navigation system loaded');
        
        // Check if we're on mobile or desktop
        const isMobile = window.innerWidth <= 768;
        
        // Mobile hamburger menu toggle (only on mobile)
        if (isMobile) {
            const mobileToggle = document.querySelector('.mobile-nav-toggle');
            const mobileNav = document.querySelector('.mobile-nav');
            
            if (mobileToggle && mobileNav) {
                mobileToggle.addEventListener('click', function() {
                    mobileNav.classList.toggle('show');
                    mobileToggle.classList.toggle('active');
                    console.log('📱 Mobile menu toggled');
                });
                
                // Close mobile menu when clicking outside
                document.addEventListener('click', function(e) {
                    if (!e.target.closest('.mobile-nav') && !e.target.closest('.mobile-nav-toggle')) {
                        mobileNav.classList.remove('show');
                        mobileToggle.classList.remove('active');
                    }
                });
            }
            
            // Mobile dropdown handling (click)
            const mobileDropdowns = document.querySelectorAll('.mobile-nav .mobile-dropdown');
            
            mobileDropdowns.forEach(function(dropdown) {
                const toggle = dropdown.querySelector('.mobile-dropdown-toggle');
                const menu = dropdown.querySelector('.mobile-dropdown-menu');
                
                if (toggle && menu) {
                    toggle.addEventListener('click', function(e) {
                        e.preventDefault();
                        
                        // Close other dropdowns
                        mobileDropdowns.forEach(function(otherDropdown) {
                            if (otherDropdown !== dropdown) {
                                const otherMenu = otherDropdown.querySelector('.mobile-dropdown-menu');
                                const otherToggle = otherDropdown.querySelector('.mobile-dropdown-toggle');
                                if (otherMenu) otherMenu.classList.remove('show');
                                if (otherToggle) otherToggle.classList.remove('active');
                            }
                        });
                        
                        // Toggle current dropdown
                        menu.classList.toggle('show');
                        toggle.classList.toggle('active');
                    });
                }
            });
        }
        
        // Desktop dropdown handling (hover) - only on desktop
        if (!isMobile) {
            const desktopDropdowns = document.querySelectorAll('.desktop-nav .nav-item.dropdown');
            
            desktopDropdowns.forEach(function(dropdown) {
                const toggle = dropdown.querySelector('.nav-link');
                const menu = dropdown.querySelector('.dropdown-menu');
                
                if (toggle && menu) {
                    let hoverTimeout;
                    
                    // Show on hover
                    dropdown.addEventListener('mouseenter', function() {
                        clearTimeout(hoverTimeout);
                        menu.classList.add('show');
                    });
                    
                    // Hide on leave with delay
                    dropdown.addEventListener('mouseleave', function() {
                        hoverTimeout = setTimeout(() => {
                            menu.classList.remove('show');
                        }, 200);
                    });
                }
            });
        }
        
        // Language switcher for footer
        window.switchLanguage = function(language) {
            if (window.languageSwitcher) {
                window.languageSwitcher.setLanguage(language);
            } else if (window.i18n && typeof window.i18n.setLanguage === 'function') {
                window.i18n.setLanguage(language);
                localStorage.setItem('aksjeradar_language', language);
                const currentLang = document.getElementById('footer-current-language');
                if (currentLang) currentLang.textContent = language.toUpperCase();
            }
        };
        
        // Cache refresh utility
        window.refreshCache = function() {
            if (typeof Storage !== "undefined") {
                localStorage.clear();
                sessionStorage.clear();
            }
            const url = new URL(window.location);
            url.searchParams.set('v', Date.now());
            url.searchParams.set('cache_refresh', 'true');
            window.location.href = url.toString();
        };
        
        console.log('✅ Clean navigation system initialized');
    });
    </script>
    
    <!-- Chart.js for data visualization -->
    <script src="https://cdn.jsdelivr.net/npm/chart.js"></script>
    
    <!-- TradingView Charts -->
    <script type="text/javascript" src="https://s3.tradingview.com/tv.js"></script>
    
    {% block extra_js %}{% endblock %}
</body>
</html>